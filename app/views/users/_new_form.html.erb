<%= form_with model: @requested_user, local: true do |f| %>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name, required: true, autofocus: true  %>
  <br>
  <%= f.label :username %>
  <br>
  <%= f.text_field :username, required: true %>
  <br>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password, required: true %>
  <br>
  <%= f.submit %>
<% end %>
