<h1>Login</h1>

<%= form_with url: sessions_path, local: true do |f| %>
  <%= f.label :username %>
  <br>
  <%= f.text_field :username, required: true, autofocus: true %>
  <br>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password, required: true %>
  <br>
  <%= f.submit "Login" %>
<% end %>
